{
    "version": 2,
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "512mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/index.py"
        }
    ]
}